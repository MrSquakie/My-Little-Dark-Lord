				float wspeed44_g1985 = temp_output_41_0_g1985;
				float3 p44_g1985 = inputMesh.positionOS;
				float3 tangent44_g1985 = tangent7_g1985;
				float3 binormal44_g1985 = binormal7_g1985;
				float3 localPWSFGerstnerWavesSRP44_g1985 = PWSFGerstnerWavesSRP( wave44_g1985 , wspeed44_g1985 , p44_g1985 , tangent44_g1985 , binormal44_g1985 );
				float4 wave45_g1985 = _Wave03;
				float wspeed45_g1985 = temp_output_41_0_g1985;
				float3 p45_g1985 = inputMesh.positionOS;
				float3 tangent45_g1985 = tangent44_g1985;
				float3 binormal45_g1985 = binormal44_g1985;
				float3 localPWSFGerstnerWavesSRP45_g1985 = PWSFGerstnerWavesSRP( wave45_g1985 , wspeed45_g1985 , p45_g1985 , tangent45_g1985 , binormal45_g1985 );

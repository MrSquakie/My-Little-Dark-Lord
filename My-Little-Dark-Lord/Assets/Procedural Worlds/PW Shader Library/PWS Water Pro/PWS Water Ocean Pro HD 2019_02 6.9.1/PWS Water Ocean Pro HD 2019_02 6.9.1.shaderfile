			PackedVaryingsMeshToPS Vert(AttributesMesh inputMesh  )
			{
				PackedVaryingsMeshToPS outputPackedVaryingsMeshToPS;

				UNITY_SETUP_INSTANCE_ID(inputMesh);
				UNITY_TRANSFER_INSTANCE_ID(inputMesh, outputPackedVaryingsMeshToPS);

				float3 ase_worldPos = GetAbsolutePositionWS( TransformObjectToWorld( (inputMesh.positionOS).xyz ) );
				float temp_output_66_0_g1985 = distance( _WorldSpaceCameraPos , ase_worldPos );
				float temp_output_82_0_g1985 = ( pow( ( temp_output_66_0_g1985 / _BeachRange ) , _BeachPower ) - _BeachOffset );
				float temp_output_85_0_g1985 = saturate( temp_output_82_0_g1985 );
				float temp_output_70_0_g1985 = ( pow( ( temp_output_66_0_g1985 / _OceanRange ) , _OceanPower ) - _OceanOffset );
				float temp_output_71_0_g1985 = saturate( temp_output_70_0_g1985 );
				float4 wave7_g1985 = _Wave01;
				float temp_output_41_0_g1985 = ( _WaveSpeed + PWSF_GlobalWindIntensityWater );
				float wspeed7_g1985 = temp_output_41_0_g1985;
				float3 p7_g1985 = inputMesh.positionOS;
				float3 tangent7_g1985 = float3(1,0,0);
				float3 binormal7_g1985 = float3(0,0,1);
				float3 localPWSFGerstnerWavesSRP7_g1985 = PWSFGerstnerWavesSRP( wave7_g1985 , wspeed7_g1985 , p7_g1985 , tangent7_g1985 , binormal7_g1985 );
